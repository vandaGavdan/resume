<div>
    <ul>
        {{#with page_header}}
        <h1 class="text-center">{{title}}</h1>
        <div class="text-center">
            <p class="text-uppercase">{{description}}</p>
        </div>
        {{/with }}
    </ul>
</div>
<br>
<hr>
<div>
    <ul>
        {{#with section}}
        <div class="card">
            <div class="card-body">

                {{#with about_us}}
                <div class="text-end">
                    {{#with title_list}}
                    <div class="display-6">{{main}}</div>
                    <h2>{{sub}}</h2>
                    {{/with }}
                </div>
                {{#with button}}
                <button class="btn btn-outline-info">{{text}}</button>
                {{/with }}
                {{/with }}
            </div>
        </div>
        <br>
        <br>

        <div class="card">
            <div class="card-body">
                {{#with advantage}}
                <div class="display-5">{{title}}</div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with efficiency}}

                        <div class="display-4">{{title}}</div>
                        <h6>{{sub_title}}</h6>
                        <p>{{description}}</p>

                        <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with }}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with innovation}}
                        <div class="text-center">
                            <div class="display-4">{{title}}</div>
                            <h6>{{sub_title}}</h6>
                            <p>{{description}}</p>
                        </div>
                        <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with }}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with collaboration}}
                        <div class="text-end">
                            <div class="display-4">{{title}}</div>
                            <h6>{{sub_title}}</h6>
                            <p>{{description}}</p>
                        </div>
                        <button class="btn btn-outline-warning">{{button}}</button>
                        {{/with }}
                    </div>
                </div>
                {{/with }}
            </div>
        </div>
        <br>
        <br>
        <div class="card">
            <div class="card-body">
                {{#with agreement}}
                <div class="display-5">{{title}}</div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with info}}
                        <h5>{{sub_title}}</h5>
                        <br>

                        {{#with detail}}
                        <div class="display-5">{{title}}</div>
                        <div class="card">
                            <p>{{text}}</p>
                            <br>
                            <button class="btn btn-success">{{button}}</button>
                        </div>
                    </div>
                    {{/with }}
                </div>
            </div>
            {{/with }}
            {{/with }}
        </div>
</div>
{{/with }}
</ul>
</div>